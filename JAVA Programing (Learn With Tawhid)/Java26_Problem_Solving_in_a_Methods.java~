import java.util.Scanner;
public class Java23_Methods
{
  // access_modifier static return_type/void method_name(parameters)
  
  public static void myMethod(int n1, String n2){
    System.out.println(n1+ " My method is working");
  }
  
  public static void main (String []args)
 {
    System.out.println("Insid main method");
    myMethod(445, "Delowar");   // methd calling (argumeents)) arguments--> parameters
    
 }
}
